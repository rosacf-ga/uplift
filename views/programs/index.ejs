<%- include('../partials/header') %> 
<div class="row">
<% programs.forEach(p => { %>
  <div class="card">
    <div class="card-title"><%= p.programName %></div>
    <div><%= p.city %>, <%= p.state %></div>
    <div>created by <%= p.user %> </div>
    <div><a href="/programs/<%= p._id %> ">Learn More</a></div>
  </div>
<% }) %> 
</div>

<%- include('../partials/footer') %>
